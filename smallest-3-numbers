#include<stdio.h>
#include<conio.h>

 void smlnum(int a,int b,int c){
    if(a<b && a<c)
    {
        printf("%d is smallest",a);
    }
   else if(b<a && b<c)
    {
        printf("%d is smallest",b);
    }
    else{
        printf("%d is smallest",c);
        
    }
}
int main(){

    int x,y,z;

    printf("enter three numbers\n");
    scanf("%d%d%d",&x,&y,&z);
    
    smlnum(x,y,z);
    
    return 0;
    
    }

 
